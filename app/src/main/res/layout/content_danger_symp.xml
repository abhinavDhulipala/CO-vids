<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back4"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".DangerSympActivity"
    tools:showIn="@layout/activity_danger_symp">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="304dp"
        android:layout_height="49dp"
        android:text="Do you have any of the following life-threatening symptoms?"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/eButton2"
        android:layout_width="366dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#C8A03636"
        android:text="Extreme difficulty breathing, gasping for air, can't talk"
        android:onClick="emergency"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Button
        android:id="@+id/eButton3"
        android:layout_width="366dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#C8A03636"
        android:onClick="emergency"
        android:text="Slurred speech (new or worsening)"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eButton2" />

    <Button
        android:id="@+id/eButton4"
        android:layout_width="366dp"
        android:layout_height="wrap_content"
        android:text="Blue Colored lips or face"
        android:backgroundTint="#C8A03636"
        android:onClick="emergency"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eButton3" />

    <Button
        android:id="@+id/eButton5"
        android:layout_width="366dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#C8A03636"
        android:text="Severe and constant pain or pressure in the chest"
        android:onClick="emergency"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eButton4" />

    <Button
        android:id="@+id/eButton6"
        android:layout_width="366dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#C8A03636"
        android:text="Slurred speech (new or worsening)"
        android:onClick="emergency"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eButton5" />

    <Button
        android:id="@+id/eButton7"
        android:layout_width="366dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#C8A03636"
        android:text="Severe and constant dizziness or lightheadedness"
        android:onClick="emergency"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eButton6" />

    <Button
        android:id="@+id/eButton8"
        android:layout_width="366dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#C8A03636"
        android:text="Acting confused (new or worsening)"
        android:onClick="emergency"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eButton7" />

    <Button
        android:id="@+id/eButton9"
        android:layout_width="366dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#C8A03636"
        android:text="Unconscious or very difficult to wake up"
        android:onClick="emergency"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eButton8" />

    <Button
        android:id="@+id/eButton10"
        android:layout_width="366dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#C8A03636"
        android:text="New seizure or seizure that won't stop"
        android:onClick="emergency"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eButton9" />

    <Button
        android:id="@+id/mildSymp"
        android:layout_width="366dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#C834B92E"
        android:text="Neither me nor a loved one is experiencing any serious symptoms"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eButton10" />
</androidx.constraintlayout.widget.ConstraintLayout>